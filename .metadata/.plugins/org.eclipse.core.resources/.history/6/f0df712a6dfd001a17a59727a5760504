package forclass;

public class test1 {

	public static void main(String[] args) {
		char arrch[] = {'D','W','O','A','B'};
		//모두 소문자로 변경한 후 출력하라
		for(int i = 0; i < arrch.length; i++) {
		
			if(arrch[i] == 'A') {
				arrch[i] = 'a';
				
				
				
			System.out.println("arrch="+arrch[i]);
		}
		}
	}
}
		
		
		
