package whileclass;

public class mainclass {

	public static void main(String[] args) {
		/*
		   
		   loop
		      for 
		            반복문                조건이 맞아야 탈출
		      for(초기화 ; 조건식; 연산식){
		                   처리
                }
               
              while
                        
                        변수의 선언
                        초기화
             while(조건식){        if(조건식)
                                   처리
                                   연산식
                 }       		  
		 */

		int w;
		w = 0;
		while(w<10) {
			System.out.println("w="+w);
			
			
			
		}
		
		
		
		
		
		
		
	}

}
