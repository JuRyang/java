package forclass;

public class test {

	public static void main(String[] args) {
		// 1~ 100사이에 있는 숫자 중 짝수들의 합, 홀수들의 합을 구하라.
		
		int sum1, sum2;
		sum1= 0;
		sum2= 0;
		
		
	for (int i =1; i <=100; i++) {
			if( i% 2 ==1) {
				sum1 = sum1+i;
			}
			else {
				sum2 = sum2+i;
				
			}
		}
		System.out.println("sum;1"+sum1);
		System.out.println("sum;2"+sum2);

	}

}
