		// TODO Auto-generated method stub
		
		/*
		  Array : 배열
		                 같은 자료형 변수들의 묶음.
		                      
		 */
		
		// 5명 학생의 국어점수 총점,평균
		int lang1, lang2, lang3, lang4,lang5;
		
		lang1 = 90;
		lang2 = 85;
		lang3 = 100;
		lang4 = 95;
		lang5 = 75;

		int sum = 0;
		
		sum = lang1 + lang2 +lang3 +lang4 + lang5;
		
		double avg = 0;
		
		avg = (double)sum / 5;
		
		

	}

}
