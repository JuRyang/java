
public class MainClass1 {

	public static void main(String[] args) {
   
		
		
		byte by;
		// 변수의 대ㄷ입
		by= 12; // value
		byte by1=24; // 선언과 동시에 대입
		
		System.out.println("by = " + by);
        by = 127;
        System.out.println("by = " + by);
        
        
        short sh; // 2byte
        sh = 1234;
        System.out.println("sh="+sh);
        
        
        
	
	
	}

}
